<?xml version="1.0" encoding="utf-8"?>
<!-- 将开关 开和关进行设定选择器 -->
<!-- 按钮的选择器，可以设置按钮在不同状态下的时候，按钮不同的颜色 -->
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_checked="true"  android:drawable="@drawable/self_site_nightmodel_switch_on" />
    <item android:drawable="@drawable/self_site_nightmodel_switch_off" />
</selector>
